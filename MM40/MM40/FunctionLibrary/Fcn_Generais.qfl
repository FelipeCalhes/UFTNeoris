	Browser("Criar requisição de compra").Page("Criar requisição de compra").SAPFrame("Criar requisição de compra").WebElement("StatusBarMessage_ReqDeCompra").WaitProperty "visible",true,4
	Set messageObj = Browser("Criar requisição de compra").Page("Criar requisição de compra").SAPFrame("Criar requisição de compra").WebElement("StatusBarMessage_ReqDeCompra")
	If Not  messageObj.Exist(3)Then
		assertFalse "Validar criação de requisição de compra","Erro na criação de ordem"
	End If
	message = messageObj.GetROProperty("innertext")
	
	numeroRequisicao = extractNumbers(message)
				
	condition = validateMessageByRegExp(message, "Requisição de compra criada sob nº \d+")
	assertCondition condition, "Validar criação de requisição de compra","Requisição de compra nº: " & numeroRequisicao & " criada com sucesso","Erro na criação de ordem"

	CriarRequisicaoDeCompraAmpliadoFiori = numeroRequisicao
End Function

Function AgregarRequisicaoDeCompraFiori(requisicaoDeCompraLiberada, fechasap)
	Browser("agregador").Page("Página inicial").Sync
	
	Browser("agregador").Page("Página inicial").WebEdit("Input_RequisicaoAgregador").Set requisicaoDeCompraLiberada
	SendKey("{ENTER}")
	wait 2
	Browser("agregador").Page("Página inicial").WebEdit("Input_RequisicaoAgregador").Click
	SendKey("{ENTER}")
	wait 2

	count = 120
	For Iterator = 1 To count
		If Browser("agregador").Page("Página inicial").WebElement("TableTitle_Requisicoes_(0)").Exist(7) Then
			numeroRequisicoes = CLng(extractNumbers(Browser("agregador").Page("Página inicial").WebElement("TableTitle_Requisicoes_(0)").GetROProperty("innertext")))
			print "Número de requisições: " & numeroRequisicoes & " - Iteração: " & Iterator
			If numeroRequisicoes > 0 Then
				Exit For
			Else
				wait 3
			End If
			
		End If
	Next

	numeroRequisicoes = extractNumbers(Browser("agregador").Page("Página inicial").WebElement("TableTitle_Requisicoes_(0)").GetROProperty("innertext"))
	
	Set requisicaoResultadoBusca = Browser("agregador").Page("Página inicial").WebElement("rowPrimeiraLinhaTabelaResultados")
	requisicaoResultadoBusca.SetTOProperty "innertext", requisicaoDeCompraLiberada
	
	condition = numeroRequisicoes = "1" and requisicaoResultadoBusca.Exist(3)
	assertCondition condition, "Validar busca da requisição nº: " & requisicaoDeCompraLiberada ,"Requisição encontrada","Requisição não encontrada"
	
	Browser("agregador").Page("Página inicial").WebElement("checkbox_FirstElementResultTable").Click
	Browser("agregador").Page("Página inicial").WebElement("Button_Agregar").Click
	
	If Browser("agregador").Page("Página inicial").WebEdit("Input_FimDaValidade").Exist(7) Then
		Browser("agregador").Page("Página inicial").WebEdit("Input_FimDaValidade").Set fechasap
		Browser("agregador").Page("Página inicial").WebElement("Button_Confirmar_FimDaValidade").Click
	End If
	
	If Browser("agregador").Page("Página inicial").WebElement("Popup_NumeroCotacao").Exist(7) Then
		mensagemPopup = Browser("agregador").Page("Página inicial").WebElement("Popup_NumeroCotacao").GetROProperty("innertext")
		condition = mensagemPopup <> ""
		print "Conteúdo popup: " & mensagemPopup
		assertCondition condition,"Validar cotação gerada","Cotação gerada com sucesso","Erro ao gerar cotação" 
	End If    
	
	numeroCotacao = extractNumbers(mensagemPopup)
	AgregarRequisicaoDeCompraFiori = numeroCotacao
End Function

